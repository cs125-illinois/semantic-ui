{
  "name": "@cs125/semantic-ui",
  "description": "CS 125 Semantic UI integrations",
  "version": "2020.5.9",
  "main": "dist/index.js",
  "files": [
    "dist/**/*",
    "semantic.min.css"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --declaration && rm -rf dist/example/ && mv dist/src/* dist && rmdir dist/src",
    "build:docs": "rm -rf docs/* && NODE_ENV=production webpack --config example/webpack.prod.config.js && rm -rf docs/*.map docs/*.js",
    "example": "webpack-dev-server --config example/webpack.dev.config.js",
    "serve": "serve -l 1234 docs",
    "prettier": "prettier --write src/ *.json && prettier-package-json --write"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cs125/react-google-login": "^2020.5.2",
    "react-children-utilities": "^2.1.1",
    "react-icons": "^3.10.0",
    "react-syntax-highlighter": "^12.2.1",
    "semantic-ui-react": "^0.88.2",
    "slugify": "^1.4.0"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.9.6",
    "@babel/preset-react": "^7.9.4",
    "@hot-loader/react-dom": "^16.13.0",
    "@mdx-js/loader": "^1.6.1",
    "@mdx-js/react": "^1.6.1",
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/webpack-env": "^1.15.2",
    "babel-loader": "^8.1.0",
    "babel-plugin-lodash": "^3.3.4",
    "babel-polyfill": "^6.26.0",
    "clean-webpack-plugin": "^3.0.0",
    "css-loader": "^3.5.3",
    "file-loader": "^6.0.0",
    "html-webpack-inline-source-plugin": "1.0.0-beta.2",
    "html-webpack-plugin": "^4.3.0",
    "lodash-webpack-plugin": "^0.11.5",
    "mini-css-extract-plugin": "^0.9.0",
    "prettier": "^2.0.5",
    "prettier-package-json": "^2.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-app-polyfill": "^1.0.6",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-hot-loader": "^4.12.21",
    "serve": "^11.3.0",
    "source-map-loader": "^0.2.4",
    "style-loader": "^1.2.1",
    "ts-loader": "^7.0.4",
    "typescript": "^3.9.2",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.7.0",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.11.0"
  },
  "prettier": {
    "semi": false,
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
