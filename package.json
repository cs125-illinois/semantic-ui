{
  "name": "@cs125/semantic-ui",
  "description": "CS 125 Semantic UI integrations",
  "version": "2020.5.31",
  "main": "dist/index.js",
  "files": [
    "dist/**/*"
  ],
  "scripts": {
    "backend": "docker-compose --env-file example/.env.development -p $npm_package_name up",
    "backend:stop": "docker-compose --env-file example/.env.development -p $npm_package_name down",
    "build": "rm -rf dist && tsc && cp -R css/dist/css dist/css",
    "check": "npm run eslint && npm run tsc && npm run prettier && npm run depcheck",
    "depcheck": "depcheck --ignore-dirs example --ignores=@types/*,@typescript-eslint/*,eslint-plugin-*",
    "eslint": "eslint --fix src/ --max-warnings=0 --ext .js,.ts,.tsx",
    "example": "cd example && gatsby develop -p 1234",
    "ncu": "ncu",
    "prepublishOnly": "npm run build",
    "start": "concurrently -n example,src,backend --handle-input \"npm run example\" \"npm run watch\" \"npm run backend\"",
    "tsc": "tsc --noEmit",
    "prettier": "prettier --write \"**/*.{ts,tsx,js,json,mdx}\" && prettier-package-json --write && cd example && prettier-package-json --write",
    "watch": "tsc --watch"
  },
  "types": "dist/index.d.ts",
  "dependencies": {
    "@cs125/element-tracker": "^2020.5.9",
    "@cs125/react-google-login": "^2020.5.5",
    "is-mobile": "^2.2.1",
    "react-children-utilities": "^2.1.1",
    "react-icons": "^3.10.0",
    "react-syntax-highlighter": "^12.2.1",
    "semantic-ui-react": "^0.88.2",
    "slugify": "^1.4.0",
    "styled-components": "^5.1.0"
  },
  "devDependencies": {
    "@types/prop-types": "^15.7.3",
    "@types/react": "^16.9.35",
    "@types/react-dom": "^16.9.8",
    "@types/react-helmet": "^6.0.0",
    "@types/react-syntax-highlighter": "^11.0.4",
    "@types/styled-components": "^5.1.0",
    "@types/throttle-debounce": "^2.1.0",
    "@typescript-eslint/eslint-plugin": "^2.34.0",
    "@typescript-eslint/parser": "^2.34.0",
    "babel-eslint": "^10.1.0",
    "concurrently": "^5.2.0",
    "depcheck": "^0.9.2",
    "eslint": "^7.0.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "eslint-plugin-react": "^7.20.0",
    "eslint-plugin-react-hooks": "^4.0.2",
    "npm-check-updates": "^6.0.1",
    "prettier": "^2.0.5",
    "prettier-package-json": "^2.1.3",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-ace": "^8.1.0",
    "react-dom": "^16.13.1",
    "typescript": "^3.9.3"
  },
  "prettier": {
    "semi": false,
    "arrowParens": "avoid",
    "trailingComma": "es5",
    "singleQuote": false,
    "printWidth": 120,
    "tabWidth": 2
  }
}
